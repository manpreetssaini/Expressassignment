<table class="table">
  <thead>
    <tr>
      <th scope="col">Description</th>
      <th scope="col">Product Name</th>
      <th scope="col">Id</th>
    </tr>
  </thead>
  <tbody>
    <%products.forEach((product) => { %>
      <tr>
        <td><%=product.description%></td>
        <td><%=product.name%></td>
				<td><%=product.id%></td>
      </tr>
    <% });%>
  </tbody>
</table>
